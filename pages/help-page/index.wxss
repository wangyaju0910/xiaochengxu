Page{
  background-color: #f5f5f5;
}

.order-set {
  background-color: #f5f5f5;
  font-size: 14px;
  color: #666;
  padding: 5px 0;
}

.order-list {
  display: flex;
  justify-content: space-between;
  font-size: 12px;
  padding: 10px 0;
}
.order-list .fukaun::after{
  content:"1";
  width: 13px;
  height:13px;
  font-size: 10px;
  background-color:#d62727;
  border-radius: 50%;
  text-align: center;
  position: absolute;
  top:130px;
  left: 50px;
  color:#fff;
}
.order-list .load-pay {
  flex: 1;
  text-align: center;
}

.order-list .load-pay .images {
  width: 20px;
  height: 20px;
  margin-left: 28px;
}

.images image {
  width: 100%;
  height: 100%;
}

.order-set .set-list {
  border-bottom: 1px solid #f5f5f5;
  padding: 10px;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
}

.set-list .icon {
  color: #bcbcbc;
}

.order-set .set-list:nth-last-child(1) {
  border: none;
}
.last-list .last-nav:nth-child(-n+3){
  border-bottom: 1px solid #f5f5f5;
}
.last-list .last-nav{
  padding: 8px 10px;
  background-color: #eeeeee;
}
.help-now{
  display: flex;
  height: 40px;
  background-color:#fff;
  position: fixed;
  bottom: 0;
  width: 100%;
  font-size: 12px;
}
.help-now .help-nav {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}
.help-now .help-nav .help-img{
  width: 20px;
  height: 20px;
}
.help-img image{
  width: 100%;
  height: 100%;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
