.username{
  border-top:1px solid #aaa;
  padding-top: 20px;
  padding-bottom: 20px;
  padding-left: 15px;
  font-size: 14px;
  height: 40px;
  line-height: 40px;
  /* float: left; */
}
.username .user-log{
  width: 140px;
  display: inline-block;
}
.username .username-img{
  width: 40px;
  height: 40px; 
  float: left;
  margin-left: 8px;
  margin-right: 5px;
}
.username .username-img image{
  width: 100%;
  height: 100%;
  border-radius: 20px;
}
.username .sign-in{
  float: right;
  margin-right: 20px;
}
.sign-in .tag-sign{
  background-color: #d62727;
  color:#fff;
  border-radius: 10px;
  line-height: 0;
  padding: 10px 10px;
  margin-top:16px;
}
.my-order .order-nav{
  display: flex;
  justify-content: space-between;
  font-size: 14px;
  padding: 10px 20px;
  border-top: 1px solid #aaa;
}
.order-nav .all-order{
  color:#999;
}
.my-order .order-list{
  display: flex;
  font-size: 14px;
  margin: 10px 0;
}
.order-list view{
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.order-set{
  background-color:#f5f5f5;
  font-size: 14px;
  color:#666;
  padding: 5px 0;
}
.order-set .set-list{
  border-bottom: 1px solid #aaa;
  padding: 10px 10px;
  background-color:#fff;
  display: flex;
  justify-content: space-between;
}
.order-set .set-list:nth-last-child(1){
    border:none;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
