.obligation{
  background-color: #333;
  color:#fff;
  font-size: 14px;
  padding:10px 0 10px 10px;
  display: flex;
  align-items: center;
}
.obligation .obligation-img{
  width: 21px;
  height: 20px;
  margin-right: 10px;
}
.obligation-img image{
  width: 100%;
  height: 100%;
}
.take-address{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 12px;
  padding: 20px 5px;
  color:#666666;
  background-color: #fff;
}
.take-address .user-address{
  flex: 5;
  display: flex
}
.user-address .fanfan{
  line-height: 20px;
}
.take-address .go-address{
  flex: 1;
  text-align: center;
  font-size: 20px;
}
.take-address .user-address .userImg{
  width: 30px;
  height: 30px;
  display: inline-block;
  margin: 0 10px;
}
.user-address .tel{
  float: right;
}
.user-address .address{
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 1;
overflow: hidden;
}
.userImg image{
  width: 100%;
  height: 100%;
}
.myOrder-list{
  background-color:#f5f5f5;
  padding: 5px 0;
}
.myOrder-list .list-nav{
  display: flex;
  /* justify-content: space-between; */
  font-size: 14px;
  background-color: #fff;
  padding: 10px 20px;
}
.list-nav .nav-img{
  width: 20px;
  height: 20px;
  margin-right: 5px;
}
.nav-img image{
  width: 100%;
  height: 100%;
}
.list-detail{
  font-size: 14px;
  background-color: #fff;
  display: flex;
  padding-bottom: 10px;
}
.list-detail .detail-img{
  width: 100px;
  height: 100px;
  flex: 1;
  margin-left: 20px;
}
.detail-img image{
  width: 100%;
  height: 100%;
}
.detail-introduce{
  flex: 2;
  margin-left: 10px;
}
.detail-introduce .introduce-money{
  color:#d62727;
  margin-top: 40px;
}
.introduce-money .num{
  float: right;
  margin-right: 20px;
}
.detail-introduce .introduce-money .number{
  font-size: 18px;
  display: inline-block;
}
.myOrder-list .aggregate{
  border-top:1px solid rgba(153, 153, 153, 0.089);
  border-bottom:1px solid rgba(153, 153, 153, 0.089);
  background-color: #fff;
  padding: 10px 0;
  font-size: 14px;
}
Page{
  background-color: #f5f5f5;
}
.order-message{
  font-size: 14px;
  color:#999;
  padding: 10px 20px;
  background-color: #fff;
}
.total-price{
  font-size: 14px;
  margin-top: 5px;
  background-color: #fff;
  width: 100%;
  padding: 10px 0px;
  text-align: right;
}
.total-price .shop-price{
  color:#999;
  margin-right: 10px;
}
.total-price .all-price{
  font-size: 16px;
  margin-right: 10px;
}
.go-pay{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 50px;
  background-color: #fff;
}
.go-pay button{
  font-size: 14px;
}
.go-pay .buttontwo{
  width: 84px;
  position: absolute;
  bottom: 5px;
  right: 10px;
  background-color: #d62727;
  color:#fff;
}
.go-pay .buttonone{
  width: 84px;
  position: absolute;
  bottom: 5px;
  right:120px;
  background-color: #fff;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}