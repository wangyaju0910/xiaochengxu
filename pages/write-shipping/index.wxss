Page{
  background-color: #f5f5f5;
}
.write-shipping{
  background-color: #fff;
  margin-top:5px;
  font-size: 14px;
  padding: 10px 10px 0 10px;
}
.write-shipping .shipping-user{
  float: left;
}
.write-shipping .shipping-tel{
  float: right;
}
.write-shipping .shipping-address{
  float: left;
  color:#c1c1c1;
  margin: 10px 0;
  padding: 10px 0;
  border-bottom: 1px solid #e3e3e3;
}
.defulte{
  display: flex;
  justify-content: space-between;
  padding: 0 10px 10px 10px;
  background-color: #fff;
  font-size: 14px;
}
.defulte .defulte-left .img{
  width: 15px;
  height:15px;
  margin-right: 8px;
}
.defulte-left{
  display: flex;
  justify-content: space-between;
}
.right-bainji{
  display: flex;
}
.defulte-left .img image{
  width: 100%;
  height: 100%;
  margin-top: 2px;
}
.right-bainji:nth-child(1){
  margin-right: 15px;
}
.left-content{
  color:#d62624;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}