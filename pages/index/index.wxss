.inputBox {
  border-top: 1px solid #dddddd;
  background-color: #fff;
}
.input{
  position: relative;
}
 .input-img{
  display: inline-block;
  width: 20px;
  height: 20px;
  margin-top: 2px;
  box-sizing: border-box;
  position: absolute;
  top:15px;
  left: 20px;
  z-index: 1;
}
.input-img image{
  width: 100%;
  height: 100%;
}
.inputBox .input {
  width: 90%;
  height: 30px;
  border: 1px solid #dddddd;
  border-radius: 8px;
  margin: 0 auto;
  margin-top: 10px;
  font-size: 14px;
  padding-left: 20px;
  color: #aaa;
  line-height: 30px;
  background-color:#f1f1f1;
  text-indent: 15px;
}
.advance-bg{
  background-color: #fff;
  padding-bottom: 5px;
}
.advance {
  width: 97%;
  border: 1px solid #ddd;
  display: flex;
  justify-content: space-between;
  font-size: 12px;
  border-radius: 8px;
  margin: 5px auto;
  align-items: center;
  padding: 5px;
  box-sizing: border-box;
  background-color: #fff;
}

.advance .user .user-img {
  width: 30px;
  height: 30px;
}

.user-img image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.advance .auction-notice{
  height: 30px;
  width: 30px;
  margin-left: 10px;
  color:#999;
}
.advance .time,
.advance .user,
.advance .theme{
  margin-left: -15px;
}
.advance .time{
  text-align: center;
  color:#d52626;
  font-weight: 600;
}
.advance .time .data{
  color:#999;
  font-size: 8px;
}
.advance-shop {
  background-color: #f5f5f5;
  margin-top: 10px;
  padding: 5px;
}

.advance-shop .advance-img {
  background-color: #fff;
  position: relative;
}

.advance-shop image {
  width: 100%;
  height: 180px;
}

.advance-shop .advance-content {
  background-color: #fff;
  padding: 0 15px;
  padding-bottom: 5px;
  font-size: 14px;
}

.advance-content .finish {
  font-size: 12px;
  color: #aaa;
  margin-top: 5px;
}

.proceed {
  position: absolute;
  bottom: 5px;
  left: 0;
  color: #fff;
  display: flex;
  justify-content: space-between;
  width: 100%;
  padding: 10px 5px;
  box-sizing: border-box;
  font-size: 12px;
  background-color: rgba(0, 0, 0, 0.6);
}
.proceed .xinImg{
  position: relative;
}
.proceed .xin{
  width: 20px;
  height: 20px;
  position: absolute;
  bottom: -2px;
}
.proceed .xin image{
  width: 100%;
  height: 100%;
}
.proceed view:nth-child(1) {
  flex: 2;
  color:#d42825;
}

.proceed view:nth-child(2) {
  flex: 1;
  text-align: center;
}
Page{
  background-color: #f5f5f5;
}